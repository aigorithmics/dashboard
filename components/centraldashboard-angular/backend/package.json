{
  "name": "kubeflow-centraldashboard",
  "version": "0.0.2",
  "description": "Kubeflow Central Dashboard",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run tslint && npm run build-ts",
    "build-ts": "tsc",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Express\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "dev-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Express\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "serve": "node dist/server.js",
    "serve-debug": "nodemon --inspect dist/server.js",
    "start": "npm run serve",
    "test": "ts-node --transpile-only node_modules/jasmine/bin/jasmine --config=app/.jasmine.json --reporter=jasmine-console-reporter",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "coverage": "nyc npm run test",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kubeflow/kubeflow.git"
  },
  "author": "Kubeflow Project",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/kubeflow/kubeflow/issues"
  },
  "vaadin": {
    "disableUsageStatistics": true
  },
  "homepage": "https://github.com/kubeflow/kubeflow#readme",
  "dependencies": {
    "@google-cloud/monitoring": "^5.3.1",
    "@kubernetes/client-node": "^1.4.0",
    "express": "^5.2.1",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/express": "^5.0.6",
    "@types/express-serve-static-core": "^4.16.9",
    "@types/gapi.client.monitoring": "^3.0.1",
    "@types/jasmine": "^3.4.2",
    "@types/node": "^25.3.0",
    "@types/node-fetch": "^2.5.2",
    "@types/serve-static": "^1.13.3",
    "@types/ws": "^8.18.1",
    "concurrently": "^7.6.0",
    "cross-env": "^5.2.1",
    "nodemon": "^3.1.14",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.8.3"
  }
}
